OPENAI_API_KEY=your_api_key_here
ANTHROPIC_API_KEY=your_api_key_here
TAVILY_API_KEY=your_api_key_here
HUGGINGFACE_API_KEY=your_api_key_here
HUGGINGFACE_ENDPOINT_URL=your_endpoint_url_here

# Embedding Model Configuration
# HuggingFace model to use for generating embeddings
# Options: BAAI/bge-base-en-v1.5, sentence-transformers/all-MiniLM-L6-v2, etc.
EMBEDDING_MODEL=BAAI/bge-base-en-v1.5

# SQLite Cache Configuration
# Path to the SQLite database file for exact match caching
CACHE_DB_PATH=cache.sqlite

# Qdrant Vector Database Configuration
# URL of the Qdrant server for semantic caching
QDRANT_URL=http://localhost:6333
# Name of the collection in Qdrant
QDRANT_COLLECTION=search_cache
# API key for Qdrant (if using cloud or authenticated instance)
# QDRANT_API_KEY=your_api_key_here
# Timeout for Qdrant operations in seconds
QDRANT_TIMEOUT=30

# Cache Behavior
# Time-to-live for cache entries in seconds (e.g., 86400 for 24 hours)
# Set to 0 or remove to disable TTL
CACHE_TTL=86400